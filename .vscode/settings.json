{
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.linting.flake8Path": "${workspaceFolder}/.venv/bin/pflake8",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.pytest_cache": true,
        "**/__pycache__": true,
        "**/.coverage": true,
        "**/coverage.xml": true
    },
    "python.linting.flake8Enabled": true,
    "python.analysis.typeCheckingMode": "basic",
    "python.formatting.provider": "black",
    "cSpell.words": [
        "absolufy",
        "backticks",
        "darkdetect",
        "datas",
        "DIYHV",
        "docstrings",
        "hioki",
        "icccm",
        "isort",
        "keysyms",
        "libegl",
        "libopengl",
        "libxcb",
        "libxkbcommon",
        "libyaml",
        "linebreak",
        "loadcell",
        "makensis",
        "numpy",
        "optosigma",
        "pflake",
        "psutil",
        "pyautolab",
        "pyinstaller",
        "pyproject",
        "pyqt",
        "pyqtdarktheme",
        "pyright",
        "pyserial",
        "pytest",
        "PYTHONIOENCODING",
        "pyupgrade",
        "qdarktheme",
        "qtawesome",
        "qtpy",
        "randr",
        "runneradmin",
        "snok",
        "UDBZ",
        "venv",
        "virtualenvs",
        "xinerama",
        "yesqa"
    ]
}
